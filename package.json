    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "17.0.2",
    "standard-version": "9.0.0",
    "ts-jest": "26.5.4",
    "ts-loader": "9.3.1",
    "ts-node": "10.4.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typedoc": "^0.17.6",
    "typedoc-plugin-markdown": "^2.2.17",
    "typescript": "3.8.3",
    "webpack": "5.74.0",
    "webpack-merge": "5.8.0",
    "winston": "3.3.3",
    "@nrwl/workspace": "latest",
    "nx": "latest",
    "@nrwl/cli": "latest",
    "@nrwl/nx-cloud": "latest"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.9.0"
  }
}
