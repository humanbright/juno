{
  "name": "@ringcentral/juno-framer",
  "version": "1.0.0",
  "license": "MIT",
  "author": "RingCentral",
  "bugs": {
    "url": "https://github.com/ringcentral/juno/issues"
  },
  "homepage": "https://github.com/ringcentral/juno#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ringcentral/juno.git"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "clean-cache": "rm -rf node_modules/.cache",
    "framer:build": "node ./scripts/esmbuild build ../juno-core/src ../../public",
    "move-dist-tmp": "mv ./index.js ../../public/index.js",
    "framer:serve": "node ./scripts/esmbuild serve ../juno-core/src 8000"
  },
  "dependencies": {
    "esbuild": "0.14.1",
    "@ringcentral/juno": "*",
    "@ringcentral/juno-icon": "*"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
